<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity WordsTest.
    -->
    <changeSet id="20181231155536-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="creator_id"
                                 baseTableName="words_test"
                                 constraintName="fk_words_test_creator_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="words_tests_id"
                                 baseTableName="words_test_word"
                                 constraintName="fk_words_test_word_words_tests_id"
                                 referencedColumnNames="id"
                                 referencedTableName="words_test"/>
        <addForeignKeyConstraint baseColumnNames="words_id"
                                 baseTableName="words_test_word"
                                 constraintName="fk_words_test_word_words_id"
                                 referencedColumnNames="id"
                                 referencedTableName="word"/>
        
    </changeSet>
</databaseChangeLog>
